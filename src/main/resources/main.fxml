    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" style="-fx-background-image: url(https://t3.ftcdn.net/jpg/00/84/71/02/360_F_84710252_ZrquscCqzOdXW00o6wMebF1TUp3wjkq9.jpg);" VBox.vgrow="ALWAYS">
         <children>
            <Label alignment="CENTER" contentDisplay="TOP" layoutX="360.0" layoutY="120.0" prefHeight="47.0" prefWidth="410.0" style="&#10;" text="Добро пожаловать в бухгалерию" textAlignment="CENTER" textFill="#f1fbfa">
