            <Button fx:id="viewAll" layoutX="559.0" layoutY="378.0" mnemonicParsing="false" onAction="#onViewButtonClick" prefHeight="31.0" prefWidth="211.0" style="-fx-background-color: #F3EBD3;" text="Показать всех сотрудников">
